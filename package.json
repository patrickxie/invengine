{
  "name": "invengine",
  "version": "0.1.0",
  "description": "new view of growth channel for apps",
  "scripts": {
    "gunicorn": "gunicorn $GUNICORN_PARAMS --chdir server entry:app",
    "start": "webpack-dev-server",
    "test-jest": "NODE_ENV=test BABEL_JEST_STAGE=0 jest",
    "test": "mocha --compilers js:babel-core/register --recursive ",
    "test:watch": "npm test -- --watch",
    "dist": "NODE_ENV=production webpack -p",
    "postinstall": "npm run dist",
    "push": "git commit -m 'GO!'&&git push -u origin dev-br"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/patrickxie/invengine.git"
  },
  "engines": {
    "node": "5.6.0",
    "npm": "3.6.0"
  },
  "keywords": [
    "react",
    "webpack",
    "growth",
    "production",
    "invite",
    "reload",
    "flask",
    "postgres",
    "share page",
    "growth channel"
  ],
  "author": "Patrick Xie",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "6.3.14",
    "faker": "3.0.1",
